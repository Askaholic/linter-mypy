{
  "name": "linter-mypy",
  "main": "./lib/init",
  "version": "2.3.0",
  "license": "MIT",
  "description": "Lint Python optional Type Hints (PEP-484) using mypy.",
  "keywords": [
    "python",
    "mypy",
    "lint",
    "PEP 484"
  ],
  "repository": "https://github.com/elarivie/linter-mypy",
  "engines": {
    "atom": ">0.50.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "npm-check-updates": "^2.10.4",
    "coffeelint": "^2.0.6"
  },
  "package-deps": [
    "linter"
  ],
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "value": "unix",
      "level": "error"
    },
    "max_line_length": {
      "value": 1024,
      "level": "error",
      "limitComments": true
    },
    "eol_last": {
      "level": "warn"
    }
  }
}
